{
  "main": "index.js",
  "version": "0.0.2",
  "scripts": {
    "android": "react-native run-android",
    "ios": "NODE_ENV=development react-native run-ios",
    "web": "expo start --web",
    "start": "react-native start",
    "format": "prettier --check ./src",
    "lint": "eslint --ext .tsx --ext .ts src/",
    "lint:fix": "eslint --ext .tsx --ext .ts src/ --fix",
    "test": "jest",
    "storybook:ios": "NODE_ENV=storybook react-native run-ios",
    "storybook": "start-storybook -p 7007",
    "prestorybook": "rnstl"
  },
  "dependencies": {
    "@react-native-community/masked-view": "^0.1.9",
    "@react-navigation/native": "^5.1.6",
    "@react-navigation/stack": "^5.2.11",
    "@types/react-native-vector-icons": "^6.4.5",
    "babel-plugin-module-resolver": "^4.0.0",
    "expo": "~37.0.3",
    "expo-updates": "~0.1.0",
    "react": "~16.9.0",
    "react-native": "^0.62.2",
    "react-native-reanimated": "^1.8.0",
    "react-native-safe-area-context": "^0.7.3",
    "react-native-screens": "^2.5.0",
    "react-native-unimodules": "~0.9.0",
    "react-native-vector-icons": "^6.6.0",
    "react-native-web": "~0.11.7"
  },
  "devDependencies": {
    "@babel/core": "~7.9.0",
    "@storybook/addon-actions": "^5.3.18",
    "@storybook/addon-links": "^5.3.18",
    "@storybook/addons": "^5.3.18",
    "@storybook/react-native": "^5.3.18",
    "@storybook/react-native-server": "^5.3.18",
    "@testing-library/jest-dom": "^5.5.0",
    "@testing-library/react": "^10.0.3",
    "@types/jest": "^25.2.1",
    "@types/react": "~16.9.23",
    "@types/react-native": "~0.61.23",
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "babel-loader": "^8.1.0",
    "babel-preset-expo": "~8.1.0",
    "eslint": "^6.8.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-native": "^3.8.1",
    "husky": "^4.2.5",
    "jest": "^25.4.0",
    "jest-expo": "~37.0.0",
    "jest-fetch-mock": "^3.0.3",
    "jest-transform-stub": "^2.0.0",
    "lint-staged": "^10.1.6",
    "prettier": "^2.0.4",
    "react-dom": "^16.13.1",
    "react-native-gesture-handler": "^1.6.1",
    "react-native-jest-mocks": "^1.5.0",
    "react-native-storybook-loader": "^1.8.1",
    "react-native-testing-library": "^1.13.0",
    "ts-jest": "^25.4.0",
    "typescript": "~3.8.3"
  },
  "jest": {
    "preset": "react-native",
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/react-native/jest/preprocessor.js",
      "\\.(ts|tsx)$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.jest.json"
      }
    },
    "setupFiles": [
      "./tests/setup.js",
      "./node_modules/react-native-gesture-handler/jestSetup.js"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint --ext .tsx --ext .ts src/ --fix"
    ],
    "./src/**": [
      "prettier --write ."
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm test"
    }
  },
  "private": true,
  "config": {
    "react-native-storybook-loader": {
      "searchDir": [
        "./src/components/"
      ],
      "pattern": "**/*.stories.tsx"
    }
  }
}
